# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'google login' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for google login
pod 'Firebase/Auth'
pod 'GoogleSignIn'
pod 'FirebaseUI'
pod 'FBSDKLoginKit'

end
